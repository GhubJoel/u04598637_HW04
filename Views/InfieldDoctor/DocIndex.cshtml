@model List<u04598637_HW04.ViewModels.InfieldDoctor>
@{
    ViewBag.Title = "DocIndex";
}


<h2>Our In-Field Doctors</h2>
<table style="width:100%;">
    <tr>
        <th>ID</th>
        <th>Hospital</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Age</th>
        <th>Gender</th>
        <th>Profession</th>
    </tr>
    @foreach (var INFDoctors in Model)
    {
        <tr>
            <td>@INFDoctors.ID</td>
            <td>@INFDoctors.Hospital</td>
            <td>@INFDoctors.FirstName</td>
            <td>@INFDoctors.LastName</td>
            <td>@INFDoctors.Age</td>
            <td>@INFDoctors.Gender</td>
            <td>@INFDoctors.Profession</td>
        </tr>
    }
</table>